{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 15,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "quotes": ["error", "single"],
    "indent": ["error", 2],
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "always"
      }
    ]
  }
}
